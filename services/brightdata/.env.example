# BrightData Service Configuration
# Copy this file to .env and fill in the values

# Server Configuration
PORT=9101

# BrightData API Configuration
BRIGHTDATA_API_KEY=
BRIGHTDATA_INSTAGRAM_DATASET_ID=
BRIGHTDATA_TIKTOK_DATASET_ID=
BRIGHTDATA_BASE_URL=https://api.brightdata.com/datasets/v3
BRIGHTDATA_MAX_URLS=50
BRIGHTDATA_MAX_CONCURRENCY=5
BRIGHTDATA_JOB_TIMEOUT=600
BRIGHTDATA_JOB_POLL_INTERVAL=5
BRIGHTDATA_FETCH_TIMEOUT=300
BRIGHTDATA_POLL_INTERVAL=30

# DeepInfra Embeddings (for reranker)
DEEPINFRA_API_KEY=
DEEPINFRA_ENDPOINT=https://api.deepinfra.com/v1/openai

# Reranker Configuration
RERANKER_ENABLED=true
RERANKER_ENDPOINT=https://api.deepinfra.com/v1/inference/Qwen/Qwen3-Reranker-8B
RERANKER_TOP_K=200

# Redis Configuration
REDIS_URL=redis://127.0.0.1:6379/0

# Firebase Emulator (for local development)
FIRESTORE_EMULATOR_HOST=localhost:9002
FIREBASE_AUTH_EMULATOR_HOST=localhost:9001
STORAGE_EMULATOR_HOST=localhost:9003
GOOGLE_CLOUD_PROJECT=penny-dev
