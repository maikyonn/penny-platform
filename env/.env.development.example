## Penny Platform - Development Environment (Example)
## Copy this file to env/.env.development and replace placeholder values with your local credentials.

# Profile & logging
APP_ENV=development
PROFILE=dev
LOG_LEVEL=debug
DEBUG=true

# Service Ports & URLs
PORT=9100
SEARCH_API_URL=http://localhost:9100
BRIGHTDATA_API_URL=http://localhost:9101
VIEWER_PORT=9102
VIEWER_ROOT_PATH=/db-viewer

# Database Configuration
DB_PATH=/absolute/path/to/DIME-AI-DB/data/lancedb
TEXT_DB_PATH=/absolute/path/to/DIME-AI-DB/data/lancedb
DIME_AI_DB_ROOT=/absolute/path/to/DIME-AI-DB
TABLE_NAME=influencer_facets
LANCEDB_STORAGE_BUCKET=penni-platform.firebasestorage.app
LANCEDB_STORAGE_OBJECT=lancedb/lancedb-snapshot.tar.gz

# Redis Configuration
REDIS_URL=redis://127.0.0.1:6379/0
RQ_JOB_TIMEOUT=900
RQ_RESULT_TTL=3600
RQ_WORKER_QUEUES=default,search,pipeline
RQ_PUBSUB_EVENTS=true
RQ_EVENTS_CHANNEL_PREFIX=jobs

# DeepInfra & Embeddings
DEEPINFRA_API_KEY=__SET_IN_SECRET_MANAGER__
DEEPINFRA_ENDPOINT=https://api.deepinfra.com/v1/openai
EMBED_MODEL=google/embeddinggemma-300m
RERANKER_ENABLED=true
RERANKER_ENDPOINT=https://api.deepinfra.com/v1/inference/Qwen/Qwen3-Reranker-8B
RERANKER_SERVICE_URL=http://localhost:9101/brightdata/rerank
RERANKER_TOP_K=200

# BrightData
BRIGHTDATA_API_KEY=__SET_IN_SECRET_MANAGER__
BRIGHTDATA_INSTAGRAM_DATASET_ID=__SET_IN_SECRET_MANAGER__
BRIGHTDATA_TIKTOK_DATASET_ID=__SET_IN_SECRET_MANAGER__
BRIGHTDATA_BASE_URL=https://api.brightdata.com/datasets/v3
BRIGHTDATA_SERVICE_URL=http://localhost:9101/brightdata/images
BRIGHTDATA_MAX_URLS=50
BRIGHTDATA_MAX_CONCURRENCY=5
BRIGHTDATA_JOB_TIMEOUT=600
BRIGHTDATA_JOB_POLL_INTERVAL=5
BRIGHTDATA_FETCH_TIMEOUT=300
BRIGHTDATA_POLL_INTERVAL=30
BRIGHTDATA_JOBS_IMMEDIATE=false

# OpenAI
OPENAI_API_KEY=__SET_IN_SECRET_MANAGER__

# Stripe (test)
STRIPE_SECRET_KEY=__SET_IN_SECRET_MANAGER__
STRIPE_WEBHOOK_SECRET=__SET_IN_SECRET_MANAGER__
STRIPE_TRIAL_DAYS=3
PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_replace_me
STRIPE_PRICE_STARTER_MONTHLY=price_test_starter
STRIPE_PRICE_PRO_MONTHLY=price_test_pro

# Gmail (dev project)
GMAIL_CLIENT_ID=__SET_IN_SECRET_MANAGER__
GMAIL_CLIENT_SECRET=__SET_IN_SECRET_MANAGER__

# Firebase / Emulators
FIREBASE_PROJECT_ID=penny-dev
GOOGLE_CLOUD_PROJECT=penny-dev
FIRESTORE_EMULATOR_HOST=localhost:9002
FIREBASE_AUTH_EMULATOR_HOST=localhost:9001
STORAGE_EMULATOR_HOST=localhost:9003
PUBSUB_EMULATOR_HOST=localhost:9005
SEARCH_FUNCTION_URL=http://127.0.0.1:5001/penny-dev/us-central1/search
FIREBASE_WEB_API_KEY=replace-with-web-api-key

# Frontend
PUBLIC_SITE_URL=http://localhost:5193

# CORS & Common
ALLOWED_ORIGINS=*
PENNY_DEFAULT_REGION=us-central1
API_V1_PREFIX=/search
